  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4A4A6A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#1DE9B6"
      }
    },
    {
      "name": "String Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#BB86FC"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#D0D0E0"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FF006E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#FF006E"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF006E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#00D9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#FF006E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attributes"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#00D9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#FF1744",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#C41E3A"
      }
    },
    {
      "name": "JSON Property Name",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#00D9FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#FF006E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#9C27B0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#00D9FF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#1DE9B6"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#4A4A6A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "CSS Property",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#D0D0E0"
      }
    },
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value.css",
        "constant.other.color.rgb-value.css"
      ],
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "name": "RegExp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#BB86FC"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#D0D0E0"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#E91E63"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#1DE9B6"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#FF1744"
      }
    },
    {
      "name": "Diff Changed",
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#FF006E"
      }
    }
  ]
}
